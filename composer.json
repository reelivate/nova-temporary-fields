{
    "name": "ganyicz/nova-temporary-fields",
    "description": "Make Laravel Nova fields temporary",
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Ganyicz",
            "email": "ganyicz.filip@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ganyicz\\NovaTemporaryFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ganyicz\\NovaTemporaryFields\\TemporaryFieldsServiceProvider"
            ]
        }
